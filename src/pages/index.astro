---
import Layout from '#layouts/Layout.astro';
import { PageContent } from './_components/PageContent';

const { url } = Astro;
const { origin } = url;

const { canonical, description, title } = {
	canonical: origin,
	description: 'Welcome to the official FancyStudio website.',
	title: 'Home',
};
---

<Layout
	canonical={canonical}
	description={description}
	title={title}
>
	<PageContent />
</Layout>
