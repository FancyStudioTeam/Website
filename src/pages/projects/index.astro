---
import Layout from '#layouts/Layout.astro';

const { url } = Astro;
const { origin } = url;

const { canonical, description, title } = {
	canonical: `${origin}/projects`,
	description:
		'Explore all the projects we have created and developed by our team.',
	title: 'Our Projects',
};
---

<Layout
	canonical={canonical}
	description={description}
	robots="index, follow"
	title={title}
></Layout>
