---
import PageContent from '#components/pages/team/PageContent.astro';
import Layout from '#layouts/Layout.astro';

const { url } = Astro;
const { origin } = url;

const { canonical, description, title } = {
	canonical: `${origin}/team`,
	description: 'Meet the members of the FancyStudio team.',
	title: 'Meet our Team',
};
---

<Layout
	canonical={canonical}
	description={description}
	robots="index, follow"
	title={title}
>
	<PageContent />
</Layout>
