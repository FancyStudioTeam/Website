{
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"#components/*": [
				"components/*"
			],
			"#layouts/*": [
				"layouts/*"
			],
			"#styles/*": [
				"styles/*"
			]
		},
		"rootDir": "src"
	},
	"exclude": [
		"dist"
	],
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	]
}
